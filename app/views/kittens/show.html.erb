<h1><%= @kitten.name %></h1>
<p>Age: <%= @kitten.age %></p>
<p>Cuteness: <%= @kitten.cuteness %></p>
<p>Softness: <%= @kitten.softness %></p>
<%= form_with(model: @kitten, method: :delete, data: { confirm: 'Are you sure you want to delete this kitten?' }) do |form| %>
  <%= form.submit 'Delete' %>
<% end %>
<%= link_to 'Back to All Kittens', kittens_path %>
